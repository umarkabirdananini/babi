            "<script>\n  function handleFeed(json) {\n    const ticker = document.getElementById(\"newsTicker\");\n    if (!json.feed || !json.feed.entry) {\n      ticker.innerHTML = `<span>🔧 No news available at the moment.</span>`;\n      return;\n    }\n\n    const entries = json.feed.entry.slice(0, 10);\n    const headlines = entries.map(entry => {\n      const title = entry.title.$t;\n      let link = \"#\";\n      if (entry.link && entry.link.length) {\n        const alternateLink = entry.link.find(l => l.rel === \"alternate\");\n        if (alternateLink) {\n          link = alternateLink.href;\n        }\n      }\n      return `<a href=\"${link}\" target=\"_blank\" style=\"color: white; text-decoration: none;\">🔹 ${title}</a>`;\n    });\n\n    ticker.innerHTML = `<span>${headlines.join(\" &nbsp; • &nbsp; \")}</span>`;\n  }\n\n  // Dynamically inject the script tag for JSONP\n  (function() {\n    const script = document.createElement('script');\n    script.src = 'https://jikanbabiradio.blogspot.com/feeds/posts/default?alt=json-in-script&callback=handleFeed';\n    document.head.appendChild(script);\n  })();\n</script>"
